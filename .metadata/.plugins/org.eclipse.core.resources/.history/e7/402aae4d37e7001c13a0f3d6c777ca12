

let map;
let coords = {lat: 30, lng: -90};

function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
        center: coords,
        zoom: 10,
        scrollwheel: false
	});
        
    let marker = new google.maps.Marker({
		position: coords,
		map: map //map object to be displayed on
		icon: "data:image/png;base64,"
    });
}